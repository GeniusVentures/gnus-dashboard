# GNUS Dashboard Environment Configuration
# Copy this file to .env.local and fill in your actual values

# API Keys
BLASTAPI_KEY=your_blastapi_key_here
WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id_here

# Ethereum Configuration
ETH_PRIVATE_KEY=your_ethereum_private_key_here
ETH_NETWORK=sepolia

# Contract Addresses
AMOY_CONTRACT=0xeC20bDf2f9f77dc37Ee8313f719A3cbCFA0CD1eB
SEPOLIA_CONTRACT=0x9af8050220D8C355CA3c6dC00a78B474cd3e3c70

# Application Configuration
NEXT_PUBLIC_BASE_URL=http://localhost:3000
NODE_ENV=development

# Security Configuration
JWT_SECRET=your_jwt_secret_here
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Monitoring and Logging
SENTRY_DSN=your_sentry_dsn_here
LOG_LEVEL=info

# Database (if needed)
DATABASE_URL=your_database_url_here